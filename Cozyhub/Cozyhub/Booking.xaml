<Window x:Class="Cozyhub.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cozyhub"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Label Content="Booking" HorizontalAlignment="Center" Height="62" Margin="0,38,0,0" VerticalAlignment="Top" Width="160" FontSize="36" FontWeight="Bold" Background="White" Foreground="#FF230282"/>
        <RadioButton Content="Wiring" HorizontalAlignment="Left" Height="36" Margin="82,191,0,0" VerticalAlignment="Top" Width="130" FontSize="20" FontWeight="Bold"/>
        <RadioButton Content="Plumbing" HorizontalAlignment="Left" Height="36" Margin="82,252,0,0" VerticalAlignment="Top" Width="130" FontSize="20" FontWeight="Bold"/>
        <RadioButton Content="Painting" HorizontalAlignment="Left" Height="36" Margin="82,314,0,0" VerticalAlignment="Top" Width="130" FontSize="20" FontWeight="Bold" Checked="RadioButton_Checked_1"/>
        <RadioButton Content="Hotel" HorizontalAlignment="Left" Height="36" Margin="82,131,0,0" VerticalAlignment="Top" Width="130" FontSize="20" FontWeight="Bold"/>
    </Grid>
</Window>
